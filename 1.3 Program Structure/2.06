	MUL B
	JMP .loop

level4:
	MOV B,A
	DIV 100
	PUSH A
	MOV A,B
	DIV 10
	CMP A,9
	JA podlevel1
 podlevel2:
	PUSH A
	MUL 10
	SUB B,A
	PUSH B
	JMP podlevel3
 podlevel1:
	SUB A,10
	PUSH A
	MOV A,B
	DIV 10
	MUL 10
	SUB B,A
	PUSH B
 podlevel3:
	POP C
	POP B
	POP A
	ADD A,48
	ADD B,48
	ADD C,48
	MOV [232],A
	MOV [233],B
	MOV [234],C
	HLT

level3:
	MOV A,49
	MOV [232],A
	HLT

error:
	JMP halflife3:
db: 
	DB "Error!'A'must be <6"
	DB 0	
halflife3:
	MOV C, db
	MOV D, 232	
Portal3:
	MOV A, [C]	
